{
  "name": "vitallens",
  "version": "0.0.1",
  "description": "VitalLens: Estimate vital signs such as heart rate and respiratory rate from video.",
  "main": "dist/vitallens.cjs.js",
  "module": "dist/vitallens.esm.js",
  "browser": "dist/vitallens.browser.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/vitallens.browser.js",
      "import": "./dist/vitallens.esm.js",
      "require": "./dist/vitallens.cjs.js"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsc && rollup -c",
    "test": "jest --coverage",
    "test:browser": "jest --selectProjects browser",
    "test:node": "jest --selectProjects node",
    "lint": "eslint src/**/*.ts",
    "start:webcam": "EXAMPLE_TO_OPEN=webcam nodemon scripts/server.cjs",
    "start:minimal": "EXAMPLE_TO_OPEN=minimal nodemon scripts/server.cjs"
  },
  "keywords": [
    "vital signs",
    "heart rate",
    "respiration",
    "rPPG",
    "video analysis",
    "healthcare",
    "remote sensing"
  ],
  "author": "Rouast Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rouast-Labs/vitallens.js.git"
  },
  "bugs": {
    "url": "https://github.com/Rouast-Labs/vitallens.js/issues"
  },
  "homepage": "https://www.rouast.com/",
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "fft.js": "^4.0.4",
    "fluent-ffmpeg": "^2.1.3",
    "node-fetch": "^2.7.0",
    "uuid": "^11.0.5",
    "ws": "^8.18.0"
  },
  "peerDependencies": {
    "@tensorflow/tfjs": "^4.20.0",
    "@tensorflow/tfjs-node": "^4.20.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-url": "^8.0.2",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jest": "^29.5.14",
    "@types/node-fetch": "^2.6.12",
    "@types/ws": "^8.5.14",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "eslint": "^8.57.0",
    "express": "^4.21.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.9",
    "puppeteer": "^23.11.1",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
