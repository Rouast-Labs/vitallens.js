{
  "name": "vitallens",
  "version": "0.1.2",
  "description": "VitalLens: Estimate vital signs such as heart rate and respiratory rate from face video.",
  "main": "./dist/vitallens.cjs.js",
  "module": "./dist/vitallens.esm.js",
  "browser": "./dist/vitallens.browser.js",
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/vitallens.browser.js",
        "types": "./dist/types/index.browser.d.ts"
      },
      "node": {
        "import": "./dist/vitallens.esm.js",
        "require": "./dist/vitallens.cjs.js",
        "types": "./dist/types/index.node.d.ts"
      }
    }
  },
  "type": "module",
  "scripts": {
    "preinstall": "node ./scripts/check-node-version.cjs",
    "build": "tsc && rollup -c",
    "prepare": "npm run build",
    "build:integration": "rollup -c --environment BUILD_INTEGRATION",
    "test": "jest --selectProjects node browser",
    "test:browser": "jest --selectProjects browser",
    "test:node": "jest --selectProjects node",
    "test:browser-integration": "npm run build:integration && jest --selectProjects browser-integration",
    "test:node-integration": "npm run build:integration && jest --selectProjects node-integration",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.{ts,js,json,css,md}\"",
    "start:browser": "EXAMPLE_TO_OPEN=browser/widget nodemon scripts/server.cjs",
    "start:browser-file": "EXAMPLE_TO_OPEN=browser/file_widget nodemon scripts/server.cjs",
    "start:browser-file-minimal": "EXAMPLE_TO_OPEN=browser/file_minimal nodemon scripts/server.cjs",
    "start:browser-webcam": "EXAMPLE_TO_OPEN=browser/webcam_widget nodemon scripts/server.cjs",
    "start:browser-webcam-minimal": "EXAMPLE_TO_OPEN=browser/webcam_minimal nodemon scripts/server.cjs",
    "start:node-file": "node ./examples/node/file.js"
  },
  "keywords": [
    "vital signs",
    "heart rate",
    "breathing",
    "respiration",
    "rPPG",
    "HR",
    "video analysis",
    "healthcare",
    "remote sensing",
    "video processing",
    "face",
    "pulse",
    "photoplethysmography",
    "ppg",
    "vitals",
    "remote photoplethysmography"
  ],
  "author": "Rouast Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rouast-Labs/vitallens.js.git"
  },
  "bugs": {
    "url": "https://github.com/Rouast-Labs/vitallens.js/issues"
  },
  "homepage": "https://www.rouast.com/",
  "files": [
    "dist/vitallens.browser.js",
    "dist/vitallens.cjs.js",
    "dist/vitallens.esm.js",
    "dist/types/index.node.d.ts",
    "dist/types/index.browser.d.ts",
    "scripts/check-node-version.cjs",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "chart.js": "^4.4.7",
    "fft.js": "^4.0.4",
    "fluent-ffmpeg": "^2.1.3",
    "mathjs": "^14.5.2",
    "semver": "^7.7.1",
    "uuid": "^11.0.5",
    "ws": "^8.18.2"
  },
  "peerDependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-core": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-url": "^8.0.2",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-core": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jest": "^29.5.14",
    "@types/puppeteer": "^7.0.4",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "express": "^5.1.0",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-puppeteer": "^11.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "puppeteer": "^24.10.0",
    "rollup": "^4.41.0",
    "rollup-plugin-string": "^3.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
