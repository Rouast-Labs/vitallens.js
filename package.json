{
  "name": "vitallens",
  "version": "0.0.1",
  "description": "VitalLens: Estimate vital signs such as heart rate and respiratory rate from video.",
  "main": "dist/vitallens.esm.js",
  "module": "dist/vitallens.esm.js",
  "browser": "dist/vitallens.browser.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vitallens.esm.js",
      "browser": "./dist/vitallens.browser.js"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsc && rollup -c",
    "start": "node examples/file.js",
    "serve": "live-server examples/webcam",
    "test": "jest --coverage && npm run lint",
    "lint": "eslint src/**/*.ts"
  },
  "keywords": [
    "vital signs",
    "heart rate",
    "respiration",
    "rPPG",
    "video analysis",
    "healthcare",
    "remote sensing"
  ],
  "author": "Rouast Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rouast-Labs/vitallens.js.git"
  },
  "bugs": {
    "url": "https://github.com/Rouast-Labs/vitallens.js/issues"
  },
  "homepage": "https://www.rouast.com/",
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "sharp": "^0.33.5"
  },
  "peerDependencies": {
    "@tensorflow/tfjs": "^4.20.0",
    "@tensorflow/tfjs-node": "^4.20.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
